---
pageable: false
blog: food
---
<% case page_type
   when 'day' %>
  <% date = Date.new(year, month, day).strftime('%b %e %Y') %>
<% when 'month' %>
  <% date = Date.new(year, month, 1).strftime('%b %Y') %>
<% when 'year' %>
  <% date = year.to_s %>
<% end %>
<%= album(page_articles, "Recipes from " + date) %>
